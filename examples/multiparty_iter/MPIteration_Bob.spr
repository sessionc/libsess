protocol MPIteration @ Bob {
  participant Alice, Charlie;

  // Alice -> ME
  String from Alice;

  // Decision happens at Alice
  repeat @ Alice {
    // Alice -> ME -> Charlie
    int from Alice;
    int to Charlie;

    // Charlie -> ME -> Alice
    int from Charlie;
    int to Alice;


    // Alice -> ME -> Charlie
    String from Alice;
    String to Charlie;
  }
}
